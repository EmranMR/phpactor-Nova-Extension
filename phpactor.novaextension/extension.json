{
  "identifier": "emran-mr.phpactor",
  "name": "PHPactor",
  "organization": "emran-mr",
  "description": "phpactor PHP Language Server",
  "version": "1.1.1",
  "categories": ["languages", "formatters", "issues"],
  "min_runtime": "10.0",
  "main": "main.js",
  "entitlements": {
    "process": true,
    "filesystem": "readwrite",
    "requests": true
  },
  "commands": {
    "extensions": [
      {
        "title": "PHPactor: Clean Cache",
        "command": "cleanCache",
        "filters": {
          "syntaxes": ["php"]
        }
      }
    ]
  },
  "activationEvents": [
    "onLanguage:php",
    "onLanguage:blade",
    "onWorkspaceContains:composer.json"
  ],

  "repository": "https://github.com/EmranMR/phpactor-Nova-Extension",
  "funding": "https://github.com/sponsors/EmranMR",
  "bugs": "https://github.com/EmranMR/phpactor-Nova-Extension/issues"
}
